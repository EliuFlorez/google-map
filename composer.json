{
    "name": "eliuflorez/google-map",
    "description": "Google Map API v3 integration for PHP 5.6+",
    "keywords": [ "google map" ],
    "license": "MIT",
    "authors": [
        {
            "name": "Eliu Florez",
            "email": "eliufz@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=5.5.9",
        "egeloen/json-builder": "^3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.7",
        "widop/http-adapter": "^1.1",
        "willdurand/geocoder": "^3.3",
        "satooshi/php-coveralls": "^2.0"
    },
	"suggest": {
        "widop/http-adapter": "Allows to use services",
        "willdurand/geocoder": "Allows to use geocoding service"
    },
    "autoload": {
        "psr-4": { "Ivory\\GoogleMap\\": "src/" }
    },
    "config": {
        "bin-dir": "bin/"
    }
}
